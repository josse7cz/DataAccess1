<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="DataAccess1" namespace="DataAccess1.Model">

  <class name="Receipt" table="receipt">
    <id name="Id" column="receipt_id">
      <generator class="native" />
    </id>
    <many-to-one name="UserName" column= "user_id" foreign-key="user_id" />
    <many-to-one name="Item" column= "item_id" foreign-key="item_id" />
  </class>

</hibernate-mapping>